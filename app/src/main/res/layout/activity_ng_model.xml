<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ng="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/activity_horizontal_margin"
    android:orientation="vertical"
    tools:context="com.ngandroid.demo.ui.pages.ngmodel.NgModelActivity"
    tools:ignore="MissingPrefix">
    
    <TextView
        android:id="@+id/tvStr"
        ng:ngModel="mod.str"
        style="@style/TextStyle"/>

    <EditText
        android:id="@+id/etStr"
        ng:ngModel="mod.str"
        style="@style/TextStyle"/>


    <TextView
        android:id="@+id/tvInteger"
        ng:ngModel="mod.integer"
        style="@style/TextStyle"/>

    <EditText
        android:id="@+id/etInteger"
        ng:ngModel="mod.integer"
        android:inputType="number"
        style="@style/TextStyle"/>

    <TextView
        android:id="@+id/tvDub"
        ng:ngModel="mod.dub"
        style="@style/TextStyle"/>

    <EditText
        android:id="@+id/etDub"
        ng:ngModel="mod.dub"
        android:inputType="numberDecimal"
        style="@style/TextStyle"/>

    <TextView
        android:id="@+id/tvFlt"
        ng:ngModel="mod.flt"
        style="@style/TextStyle"/>

    <EditText
        android:id="@+id/etFlt"
        ng:ngModel="mod.flt"
        android:inputType="numberDecimal"
        style="@style/TextStyle"/>

    <TextView
        android:id="@+id/tvLon"
        ng:ngModel="mod.lon"
        style="@style/TextStyle"/>

    <EditText
        android:id="@+id/etLon"
        ng:ngModel="mod.lon"
        android:inputType="numberDecimal"
        style="@style/TextStyle"/>



</LinearLayout>
